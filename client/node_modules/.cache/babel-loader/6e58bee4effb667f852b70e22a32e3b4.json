TH,EAUGJ,KAVH,CAUS,UAACzB,KAAD,EAAW;AAChB,cAAI,CAACF,cAAL,CAAoBE,KAAK,CAAC8B,QAAN,CAAezD,IAAf,CAAoB0B,OAAxC;AACD,OAZH;AAaD;AAtEM;AA9DI,CAAf,C,CAuIA","names":["axios","ref","useRouter","useToast","firebase","name","data","maxChar","maxCharLong","toastMessage","toastColor","firstName","lastName","phone","setup","toast","router","email","password","role","confirmPassword","computed","comparePasswords","watch","isLoggedIn","handler","newValue","$root","loggedIn","immediate","methods","showErrorToast","message","error","position","duration","action","text","onClick","e","toastObject","goAway","showToast","success","autoClose","hideProgressBar","closeOnClick","pauseOnHover","draggable","progress","undefined","register","auth","createUserWithEmailAndPassword","then","user","createUser","uid","catch","sendMentorData","userID","post","showFirst","response"],"sourceRoot":"","sources":["/Users/nihatmuradzade/Desktop/Desktop/Work/hirelamp/client/src/components/SignUpMentor1.vue"],"sourcesContent":["<template>\n  <!-- component -->\n          <div>\n            <form @submit.prevent=\"sendMentorData\">\n              <div class=\"flex -mx-3\">\n                <div class=\"w-1/2 px-3 mb-4\">\n                  <!-- <label for=\"\" class=\"text-xs font-semibold px-1\"\n                  >First name</label\n                > -->\n                  <div class=\"flex\">\n                    <div\n                      class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                    >\n                      <i\n                        class=\"mdi mdi-account-outline text-gray-400 text-lg\"\n                      ></i>\n                    </div>\n                    <input\n                      type=\"text\"\n                      v-model=\"firstName\"\n                      class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                      placeholder=\"First name\"\n                      required\n                    />\n                  </div>\n                </div>\n                <div class=\"w-1/2 px-3 mb-4\">\n                  <!-- <label for=\"\" class=\"text-xs font-semibold px-1\"\n                  >Last name</label\n                > -->\n                  <div class=\"flex\">\n                    <div\n                      class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                    >\n                      <i\n                        class=\"mdi mdi-account-outline text-gray-400 text-lg\"\n                      ></i>\n                    </div>\n                    <input\n                      type=\"text\"\n                      v-model=\"lastName\"\n                      class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                      placeholder=\"Last name\"\n                      required\n                    />\n                  </div>\n                </div>\n              </div>\n              <div class=\"flex -mx-3\">\n                <div class=\"w-full px-3 mb-4\">\n                  <!-- <label for=\"\" class=\"text-xs font-semibold px-1\">Email</label> -->\n                  <div class=\"flex\">\n                    <div\n                      class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                    >\n                      <i\n                        class=\"mdi mdi-email-outline text-gray-400 text-lg\"\n                      ></i>\n                    </div>\n                    <input\n                      type=\"email\"\n                      class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                      placeholder=\"Email\"\n                      required\n                      v-model=\"email\"\n                    />\n                  </div>\n                </div>\n              </div>\n\n              <div class=\"flex -mx-3\">\n                <div class=\"w-full px-3 mb-4\">\n                  <!-- <label for=\"\" class=\"text-xs font-semibold px-1\">Email</label> -->\n                  <div class=\"flex\">\n                    <div\n                      class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                    >\n                      <i\n                        class=\"mdi mdi-email-outline text-gray-400 text-lg\"\n                      ></i>\n                    </div>\n                    <input\n                      required\n                      type=\"number\"\n                      class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                      placeholder=\"Phone number\"\n                      v-model=\"phone\"\n                    />\n                  </div>\n                </div>\n              </div>\n\n              <div class=\"flex -mx-3\">\n                <div class=\"w-full px-3 mb-4\">\n                  <div class=\"flex\">\n                    <div\n                      class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                    >\n                      <i class=\"mdi mdi-lock-outline text-gray-400 text-lg\"></i>\n                    </div>\n                    <input\n                      type=\"password\"\n                      class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                      placeholder=\"Password\"\n                      required\n                      v-model=\"password\"\n                    />\n                  </div>\n                </div>\n              </div>\n\n              <div class=\"flex -mx-3\">\n                <div class=\"w-full px-3 mb-4\">\n                  <div class=\"flex\">\n                    <div\n                      class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                    >\n                      <i class=\"mdi mdi-lock-outline text-gray-400 text-lg\"></i>\n                    </div>\n                    <input\n                      type=\"password\"\n                      class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                      placeholder=\"Confirm Password\"\n                      required\n                      v-model=\"confirmPassword\"\n                      :rules=\"[comparePasswords]\"\n                    />\n                  </div>\n                </div>\n              </div>\n\n              <div class=\"flex -mx-3\">\n                <div class=\"w-full px-3 mb-4\">\n                  <button\n                    @click=\"register\"\n                    type=\"submit\"\n                    value=\"Register\"\n                    class=\"block w-full shadow-md border-3 border-blue-400 max-w-xs mx-auto bg-custom-blue hover:bg-blue-500 focus:bg-blue-700 text-white rounded-2xl px-3 py-2 font-semibold\"\n                  >\n                    NEXT\n                  </button>\n                </div>\n              </div>\n\n              <div class=\"flex -mx-3\">\n                <div class=\"w-full px-3 mb-4\">\n                  <button\n                    class=\"block w-full max-w-xs mx-auto hover:text-custom-blue px-3 py-2 font-semibold\"\n                  >\n                    <a href=\"/login\">Already have an account? Login</a>\n                  </button>\n                </div>\n              </div>\n            </form>\n          </div>\n     \n</template>\n\n<script>\nimport axios from \"axios\";\nimport { ref } from \"vue\";\nimport { useRouter } from \"vue-router\"; // import router\nimport { useToast } from \"vue-toastification\";\nimport firebase from \"firebase/compat/app\";\n\nexport default {\n  name: \"HomePage\",\n  // props: {\n  //  loggedIn: {\n  //    type: Boolean,\n  //    required: true\n  //  }\n  // },\n  data() {\n    return {\n      maxChar: 50,\n      maxCharLong: 500,\n      toastMessage: \"\",\n      toastColor: \"\",\n      firstName: \"\",\n      lastName: \"\",\n      phone: \"\",\n    };\n  },\n  setup() {\n    const toast = useToast();\n    const router = useRouter();\n    const email = ref(\"\");\n    const password = ref(\"\");\n    const role = ref(\"\");\n    const confirmPassword = ref(\"\");\n    const phone = ref(\"\");\n\n    return {\n      role,\n      toast,\n      router,\n      email,\n      password,\n      confirmPassword,\n      phone,\n    };\n  },\n\n  computed: {\n    comparePasswords() {\n      return this.password !== this.confirmPassword\n        ? \"Passwords do not match\"\n        : true;\n    },\n   \n  },\n\n  watch: {\n    isLoggedIn: {\n      handler(newValue) {\n        if (newValue) {\n      return this.$root.loggedIn == true;\n\n    \n        }\n      },\n      immediate: true,\n    },\n    \n  },\n\n  methods: {\n    showErrorToast(message) {\n      this.toast.error(message, {\n        position: \"top-right\",\n        duration: 3000,\n        action: {\n          text: \"Dismiss\",\n          onClick: (e, toastObject) => {\n            toastObject.goAway(0);\n          },\n        },\n      });\n    },\n\n    showToast() {\n      this.toast.success(\"Account created successfully!\", {\n        position: \"top-right\",\n        autoClose: 5000,\n        hideProgressBar: false,\n        closeOnClick: true,\n        pauseOnHover: true,\n        draggable: true,\n        progress: undefined,\n      });\n    },\n\n    register() {\n      if (this.password !== this.confirmPassword) {\n        this.showErrorToast(\"Passwords do not match\");\n      } else {\n        firebase\n          .auth()\n          .createUserWithEmailAndPassword(this.email, this.password)\n          .then((user) => {\n            this.showToast();\n            this.register = true;\n            this.createUser();\n            this.$root.uid = user.uid;\n           this.isLoggedIn = true;\n\n\n          })\n          .catch((error) => {\n            this.showErrorToast(error.message);\n          });\n      }\n    },\n    sendMentorData() {\n      const user = {\n        email: this.email,\n        firstName: this.firstName,\n        lastName: this.lastName,\n        // password: this.password,\n        userID: this.$root.uid,\n        role: this.role,\n      };\n\n      axios\n        .post(\n          \"https://2d13ac092947-hirelamp-bbcf628a86ebae0f2646300d98508d5.co/mentorCreate/\",\n          user\n        )\n        .then(() => {\n                     this.$root.loggedIn = true;\n                     this.$root.showFirst = false\n\n        })\n        .catch((error) => {\n          this.showErrorToast(error.response.data.message);\n        });\n    },\n  },\n};\n// get a reference to our vue router\n</script>\n\n<style></style>\n"]}]}