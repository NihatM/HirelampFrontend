ck","e","toastObject","goAway","showToast","success","autoClose","hideProgressBar","closeOnClick","pauseOnHover","draggable","progress","undefined","register","auth","createUserWithEmailAndPassword","then","user","createUser","$root","uid","$router","push","catch","userID","post","response"],"sourceRoot":"","sources":["/Users/nihatmuradzade/Desktop/Desktop/Work/hirelamp/client/src/pages/SignUp.vue"],"sourcesContent":["<template>\n  <!-- component -->\n\n  <div class=\"flex items-center justify-center\">\n    <div\n      class=\"bg-gray-200 text-gray-500 shadow-xl w-full h-screen overflow-hidden md:pt-10\"\n    >\n      <div class=\"md:flex w-full justify-center\">\n        <!-- <img\n          src=\"../assets/decoration.png\"\n          alt=\"\"\n          class=\"h-100 w-100 md:h-50 md:w-50 xl:w-50 xl:h-50 lg:w-50 lg:h-50\"\n        /> -->\n        <!-- <div class=\"md:flex justify-center hidden md:visible\"> -->\n\n        <!-- <div class=\"hidden md:visible md:block w-1/2 py-10 px-10\">\n          <img\n            src=\"../assets/SignUp.svg\"\n            alt=\"\"\n            class=\"h-100 w-100 md:h-50 md:w-50 xl:w-50 xl:h-50 lg:w-50 lg:h-50\"\n          />\n        </div> -->\n\n        <div class=\"w-full md:w-1/2 py-3 px-5 md:px-10\">\n          <div class=\"text-center mb-4\">\n            <h1 class=\"font-bold text-3xl text-gray-900\">REGISTER</h1>\n            <p>Enter your information to register</p>\n          </div>\n          <div>\n            <div class=\"flex -mx-3\">\n              <div class=\"w-1/2 px-3 mb-4\">\n                <!-- <label for=\"\" class=\"text-xs font-semibold px-1\"\n                  >First name</label\n                > -->\n                <div class=\"flex\">\n                  <div\n                    class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                  >\n                    <i\n                      class=\"mdi mdi-account-outline text-gray-400 text-lg\"\n                    ></i>\n                  </div>\n                  <input\n                    type=\"text\"\n                    v-model=\"firstName\"\n                    class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                    placeholder=\"First name\"\n                    required\n                  />\n                </div>\n              </div>\n              <div class=\"w-1/2 px-3 mb-4\">\n                <!-- <label for=\"\" class=\"text-xs font-semibold px-1\"\n                  >Last name</label\n                > -->\n                <div class=\"flex\">\n                  <div\n                    class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                  >\n                    <i\n                      class=\"mdi mdi-account-outline text-gray-400 text-lg\"\n                    ></i>\n                  </div>\n                  <input\n                    type=\"text\"\n                    v-model=\"lastName\"\n                    class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                    placeholder=\"Last name\"\n                    required\n                  />\n                </div>\n              </div>\n            </div>\n            <div class=\"flex -mx-3\">\n              <div class=\"w-full px-3 mb-4\">\n                <!-- <label for=\"\" class=\"text-xs font-semibold px-1\">Email</label> -->\n                <div class=\"flex\">\n                  <div\n                    class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                  >\n                    <i class=\"mdi mdi-email-outline text-gray-400 text-lg\"></i>\n                  </div>\n                  <input\n                    type=\"email\"\n                    class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                    placeholder=\"Email\"\n                    required\n                    v-model=\"email\"\n                  />\n                </div>\n              </div>\n            </div>\n            <div class=\"flex -mx-3\">\n              <div class=\"w-full px-3 mb-4\">\n                <!-- <label for=\"\" class=\"text-xs font-semibold px-1\"\n                  >Password</label\n                > -->\n                <div class=\"flex\">\n                  <div\n                    class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                  >\n                    <i class=\"mdi mdi-lock-outline text-gray-400 text-lg\"></i>\n                  </div>\n                  <input\n                    type=\"password\"\n                    class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                    placeholder=\"Password\"\n                    required\n                    v-model=\"password\"\n                  />\n                </div>\n              </div>\n            </div>\n\n            <div class=\"flex -mx-3\">\n              <div class=\"w-full px-3 mb-4\">\n                <!-- <label for=\"\" class=\"text-xs font-semibold px-1\"\n                  >Confirm Password</label\n                > -->\n                <div class=\"flex\">\n                  <div\n                    class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                  >\n                    <i class=\"mdi mdi-lock-outline text-gray-400 text-lg\"></i>\n                  </div>\n                  <input\n                    type=\"password\"\n                    class=\"w-full -ml-10 pl-5 pr-3 py-2 shadow-md rounded-2xl border-2 border-gray-200 outline-none focus:border-indigo-500\"\n                    placeholder=\"Confirm Password\"\n                    required\n                    v-model=\"confirmPassword\"\n                    :rules=\"[comparePasswords]\"\n                  />\n                </div>\n              </div>\n            </div>\n            <!-- \n            <div class=\"flex -mx-3\">\n              <div class=\"w-full px-3 mb-8\">\n\n                <div class=\"flex\">\n                  <div\n                    class=\"w-10 z-10 pl-1 text-center pointer-events-none flex items-center justify-center\"\n                  >\n                    <i class=\"mdi mdi-lock-outline text-gray-400 text-lg\"></i>\n                  </div>\n                  <select\n                    class=\"form-control rounded-2xl w-full shadow-md -ml-10 pl-5 pr-3 py-2 border-2 border-gray-200 text-gray-400 outline-none focus:border-indigo-500\"\n                    id=\"role\"\n                    name=\"role\"\n                    required\n                  >\n                    <option value=\"\">Select role</option>\n                    <option value=\"mentor\">Mentor</option>\n                    <option value=\"mentee\">Mentee</option>\n                  </select>\n                </div>\n              </div>\n            </div> -->\n\n            <!-- <div class=\"form-group\">\n              <label for=\"role\">Role</label>\n              <select\n                class=\"form-control rounded-2xl\"\n                id=\"role\"\n                name=\"role\"\n                required\n              >\n                <option value=\"\">Select role</option>\n                <option value=\"mentor\">Mentor</option>\n                <option value=\"mentee\">Mentee</option>\n              </select>\n            </div> -->\n\n            <div class=\"flex -mx-3\">\n              <div class=\"w-full px-3 mb-4\">\n                <button\n                  @click=\"register\"\n                  type=\"submit\"\n                  value=\"Register\"\n                  class=\"block w-full shadow-md border-3 border-blue-400 max-w-xs mx-auto bg-custom-blue hover:bg-blue-500 focus:bg-blue-700 text-white rounded-2xl px-3 py-2 font-semibold\"\n                >\n                  REGISTER NOW\n                </button>\n              </div>\n            </div>\n\n            <div class=\"flex -mx-3\">\n              <div class=\"w-full px-3 mb-4\">\n                <button\n                  class=\"block w-full max-w-xs mx-auto hover:text-custom-blue px-3 py-2 font-semibold\"\n                >\n                  <a href=\"/login\">Already have an account? Login</a>\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { ref } from \"vue\";\nimport { useRouter } from \"vue-router\"; // import router\nimport { useToast } from \"vue-toastification\";\nimport firebase from \"firebase/compat/app\";\n\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      role: \"\",\n      toastMessage: \"\",\n      toastColor: \"\",\n      firstName: \"\",\n      lastName: \"\",\n    };\n  },\n  setup() {\n    const toast = useToast();\n    const router = useRouter();\n    const email = ref(\"\");\n    const password = ref(\"\");\n    const confirmPassword = ref(\"\");\n\n    return {\n      toast,\n      router,\n      email,\n      password,\n      confirmPassword,\n    };\n  },\n\n  computed: {\n    comparePasswords() {\n      return this.password !== this.confirmPassword\n        ? \"Passwords do not match\"\n        : true;\n    },\n  },\n\n  methods: {\n    showErrorToast(message) {\n      this.toast.error(message, {\n        position: \"top-right\",\n        duration: 3000,\n        action: {\n          text: \"Dismiss\",\n          onClick: (e, toastObject) => {\n            toastObject.goAway(0);\n          },\n        },\n      });\n    },\n\n    showToast() {\n      this.toast.success(\"Account created successfully!\", {\n        position: \"top-right\",\n        autoClose: 5000,\n        hideProgressBar: false,\n        closeOnClick: true,\n        pauseOnHover: true,\n        draggable: true,\n        progress: undefined,\n      });\n    },\n\n    register() {\n      if (this.password !== this.confirmPassword) {\n        this.showErrorToast(\"Passwords do not match\");\n      } else {\n        firebase\n          .auth()\n          .createUserWithEmailAndPassword(this.email, this.password)\n          .then((user) => {\n            this.showToast();\n            this.register = true;\n            this.createUser();\n            this.$root.uid = user.uid;\n            this.$router.push(\"/mentorpage\");\n          })\n          .catch((error) => {\n            this.showErrorToast(error.message);\n          });\n      }\n    },\n    createUser() {\n      const user = {\n        email: this.email,\n        firstName: this.firstName,\n        lastName: this.lastName,\n        password: this.password,\n        userID: this.$root.uid,\n      };\n      axios\n        .post(\n          \"https://2d13ac092947-hirelamp-bbcf628a86ebae0f2646300d98508d5.co/mentees/\",\n          user\n        )\n        .then(() => {\n          this.router.push(\"/mentorpage\");\n        })\n        .catch((error) => {\n          this.showErrorToast(error.response.data.message);\n        });\n    },\n  },\n};\n// get a reference to our vue router\n</script>\n\n<style></style>\n"]}]}