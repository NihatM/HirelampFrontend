ine-block text-left\">\n            <div>\n              <button\n                @click=\"showDropdown = !showDropdown\"\n                type=\"button\"\n                class=\"inline-flex truncate w-full justify-center rounded-md px-4 py-2 text-gray-800 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-custom-blue focus:ring-offset-2 focus:ring-offset-gray-100\"\n                id=\"menu-button\"\n                aria-expanded=\"true\"\n                aria-haspopup=\"true\"\n              >\n                {{ this.fullName }}\n                <!-- Heroicon name: mini/chevron-down -->\n                <svg\n                  class=\"-mr-1 ml-2 h-5 w-5\"\n                  xmlns=\"http://www.w3.org/2000/svg\"\n                  viewBox=\"0 0 20 20\"\n                  fill=\"currentColor\"\n                  aria-hidden=\"true\"\n                >\n                  <path\n                    fill-rule=\"evenodd\"\n                    d=\"M5.23 7.21a.75.75 0 011.06.02L10 11.168l3.71-3.938a.75.75 0 111.08 1.04l-4.25 4.5a.75.75 0 01-1.08 0l-4.25-4.5a.75.75 0 01.02-1.06z\"\n                    clip-rule=\"evenodd\"\n                  />\n                </svg>\n              </button>\n            </div>\n\n            <div\n              :class=\"showDropdown ? 'block' : 'hidden'\"\n              class=\"absolute right-0 z-10 mt-2 w-56 origin-top-right rounded-md bg-white shadow-lg ring-1 ring-black ring-opacity-5 focus:outline-none\"\n              role=\"menu\"\n              aria-orientation=\"vertical\"\n              aria-labelledby=\"menu-button\"\n              tabindex=\"-1\"\n            >\n              <div class=\"py-1\" role=\"none\">\n                <!-- Active: \"bg-gray-100 text-gray-900\", Not Active: \"text-gray-700\" -->\n                <a\n                  href=\"/dashboard\"\n                  class=\"text-gray-700 block px-4 py-2 text-sm\"\n                  role=\"menuitem\"\n                  tabindex=\"-1\"\n                 