ull xl:w-1/2 md:mb-14 xl:mb-0 relative flex items-center justify-center\"\n    >\n      <img\n        src=\"../assets/interview_new.svg\"\n        alt=\"\"\n        class=\"md:h-100 lg:h-100 xl:h-100 md:w-1/2 xl:w-w-full lg:w-full\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\n// import axios from \"axios\";\nimport \"../assets/main.css\";\nexport default {\n  name: \"Search\",\n\n  data() {\n    return {\n      maxChar: 50,\n      getMentorDetails: false,\n      mentorDatas: [],\n      search: \"\",\n    };\n  },\n\n  methods: {\n    searchMentor() {\n      console.log(this.search);\n      this.$router.push(`/search/${this.search}/`);\n      localStorage.setItem(\"query\", this.search);\n    },\n  },\n};\n</script>\n\n<style>\n.jaja {\n  font: \"Biotif\";\n  background: red;\n}\n</style>\n"]}]}